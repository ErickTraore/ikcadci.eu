A,CAAA,KAAA,IAAA,CAAA,QAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,iFAAA;AACA;;AAEA,UAAA,CAAA,KAAA,IAAA,CAAA,KAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,4BAAA;AACA,OAFA,MAEA,IAAA,CAAA,KAAA,UAAA,CAAA,KAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,gCAAA;AACA;;AAEA,UAAA,CAAA,KAAA,MAAA,CAAA,MAAA,EAAA;AACA,eAAA,KAAA,IAAA,CAAA,KAAA,IAAA,CAAA;AACA;AAEA,KAvBA;AAyBA,IAAA,IAAA,EAAA,UAAA,IAAA,EAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,wCAAA,EAAA,IAAA,EACA,IADA,CACA,OAAA,IAAA;AACA,aAAA,IAAA,GAAA,OAAA,CAAA,IAAA,CADA,CAEA;;AACA,YAAA,YAAA,GAAA;AACA,UAAA,IAAA,EAAA,KAAA,IAAA,CAAA,OADA;AAEA,UAAA,OAAA,EAAA,KAAA,IAAA,CAAA;AAFA,SAAA;AAIA,YAAA,YAAA,GAAA,IAAA,CAAA,SAAA,CAAA,YAAA,CAAA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,KAAA,EAAA,YAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,WAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAZA,EAcA,KAdA,CAcA,KAAA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,CAdA;AAeA,KAzCA;AA2CA,IAAA,UAAA,EAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,GAAA,uJAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AAEA,KA/CA;AAgDA,IAAA,aAAA,EAAA,UAAA,QAAA,EAAA;AACA,YAAA,KAAA,GAAA,kBAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA;AACA;AAnDA;AAXA,CAAA","sourcesContent":["<template>\n    <div class='group__header'>\n       <div>CONEXION</div>\n        <form @submit=\"postData\" method=\"post\">\n            <div class='group__header__body'>\n                <p v-if=\"errors.length\">\n                    <b>Merci de corriger les erreurs suivantes : </b>\n                <ul>\n                    <li v-for=\"error in errors\" :key=\"error\">{{ error }}</li>\n                </ul>\n                </p>\n\n              \n                <p>\n                    <label for=\"email\">Email* </label>\n                    <input\n                            id=\"email\"\n                            v-model=\"user.email\"\n                            type=\"email\"\n                            name=\"email\"\n                    >\n                </p>\n                <p>\n                    <label for=\"password\">Mot de passe* </label>\n                    <input\n                            id=\"password\"\n                            v-model=\"user.password\"\n                            type=\"password\"\n                            name=\"password\"\n                    >\n                </p>\n              \n                <button class=\"btn\">Conexion</button>\n            </div>\n        </form>\n      <div>Vous n'avez pas de compte:</div>\n     <div>\n       <a class=\"labelForm\" href=\"/Singnup#/signup\">Créer un compte</a>\n\n       </div>\n    </div>\n</template>\n\n<script>\n  import Vue from \"vue\";\n  \n  import VueAxios from \"vue-axios\";\n  import axios from \"axios\";\n\n  Vue.use(VueAxios, axios)\n  export default {\n    name: 'ConexionComponent',\n    data() {\n      return {\n        errors: [],\n        user: {\n          email: null,\n          password: null,\n        }\n      }\n    },\n    methods: {\n      postData: function (e) {\n\n        e.preventDefault();\n\n        this.errors = [];\n\n        if (!this.user.password) {\n          this.errors.push('Veillez saisir un mot de passe');\n        } else if (!this.validPassword(this.user.password)) {\n          this.errors.push('Votre mot de passe doit contenir entre 4 et 8 caractères et au moins un chiffre');\n        }\n\n        if (!this.user.email) {\n          this.errors.push('Veillez saisir votre email');\n        } else if (!this.validEmail(this.user.email)) {\n          this.errors.push('L\\'adresse email est invalide.');\n        }\n\n        if (!this.errors.length) {\n          return this.post(this.user);\n        }\n\n      },\n\n      post: function (user) {\n         this.axios.post('http://localhost:3000/api/users/login/',user)\n          .then(reponse => {\n            this.user = reponse.data\n                    // stokage token dans localStorage\n                    let objMySession = {\n                      myId: this.user.$userId,\n                      myToken: this.user.$token\n                      }\n                    let sessionStore = JSON.stringify(objMySession);\n                    localStorage.setItem(\"obj\",sessionStore);\n                    this.$store.dispatch(\"nowLogged\")\n                    this.$router.push({path: '/'});\n                    })\n\n          .catch(error => console.log(error()))\n      },\n\n      validEmail: function (email) {\n        const regex = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n        return regex.test(email);\n\n      },\n      validPassword: function (password) {\n        const regex = /^(?=.*\\d).{4,8}$/;\n        return regex.test(password);\n      }\n    }\n  }\n\n\n</script>\n<style scoped>\n    * {\n        box-sizing: border-box;\n    }\n\n    .group__header {\n        font-size: 1.2rem;\n        align-items: center;\n        padding: 1rem;\n        border-radius: 2rem;\n        color: white;\n          background-color: rgb(35, 49, 82);\n\n\n\n    }\n\n    .group__header__body {\n        padding: 1rem;\n        background-color: rgba(196, 192, 192, 0.849);\n\n        border-radius: 2rem;\n        margin-top: 1rem;\n        margin-bottom: 1rem;\n    }\n     .labelForm {\n     color:white;\n   }\n</style>"],"sourceRoot":"src/components"}]}