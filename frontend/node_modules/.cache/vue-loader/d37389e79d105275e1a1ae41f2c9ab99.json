" :key=\"item .id\"> -->\n              <!-- <tr v-for=\"item in items | paginate\"> -->\n                <div>\n                    <b>{{ item .User.username }}</b> à écrit le\n                    {{ new Date(item .createdAt) | dateFormat('DD/MM/YYYY') }} à\n                    {{ new Date(item .createdAt) | dateFormat('hh:mm') }} : <br>\n                    <b>{{ item .title }}</b><br>\n                    {{ item .content }}<br>                  <!-- <u>Nombre de like:</u> {{ message.likes }} -->\n                </div>\n                <div _ngcontent-cpa-c6=\"\" class=\"like-buttons\">\n                    <div _ngcontent-cpa-c6=\"\" class=\"likes\">\n                        <button\n                                v-on:click=\"doLike(item .id)\">\n                            <i _ngcontent-cpa-c6=\"\" class=\"like fa-thumbs-up fa-lg far\"></i>\n                        </button>\n                        <span _ngcontent-cpa-c6=\"\">{{ item .likes }}</span>\n\n                    </div>\n                    <div _ngcontent-cpa-c6=\"\" class=\"dislikes\">\n                        <button\n                                v-on:click=\"doDislike(item .id)\">\n                            <i _ngcontent-cpa-c6=\"\" class=\"dislike fa-thumbs-down fa-lg far\"></i>\n                        </button>\n                        <span _ngcontent-cpa-c6=\"\">{{ item .dislikes }}</span>\n\n                    </div>\n                </div>\n                 <ul>\n                    <!-- <span class=\"show\" v-for=\"item in list\">{{item}}</span> -->\n                    <button type=\"button\" @click=\"reset\">reset</button>\n                    <pagination :page-data.sync=\"messages\" url=\"/api/getData\" name=\"first\"></pagination>\n                  </ul>\n            </div>\n        \n            <div class='group__header__body'>\n                <form @submit=\"postData\" method=\"post\">\n                    <label>Nouveau message</label> <br> <br>\n                    <input\n                            id=\"title\"\n                            v-model=\"posts.title\"\n                            type=\"text\"\n                            name=\"title\"\n                            placeholder=\"Titre\"\n                    > <br> <br>\n\n                    <input\n                            id=\"content\"\n                            v-model=\"posts.content\"\n                            type=\"text\"\n                            name=\"content\"\n                            placeholder=\"Contenu\"\n                    > <br> <br>\n\n                    <button type=\"submit\">Envoyer</button>\n                </form>\n            </div>\n        </div>\n    </div>\n</template>\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/vue/1.0.16/vue.js\"></script>\n<script>\n  import Vue from 'vue'\n  import axios from 'axios';\n  import VueAxios from 'vue-axios'\n  import VueFilterDateFormat from '@vuejs-community/vue-filter-date-format';\n  import Resource from 'vue-resource'\n  import Pagination from 'vue-pagination'\n\n  Vue.use(Pagination);\n  Vue.use(VueFilterDateFormat);\n  Vue.use(VueAxios, axios)\n\n  let page = new Pagination({\n    pageSize: 10,\n    remote: {\n        pageIndexName: 'pageIndex',\n        pageSizeName: 'pageSize',\n        params: {},\n        url: '', //\n        totalName: 'total',\n        offset: -1,\n        dataKey: 'data'\n    }\n});\n\npage.init();\n  export default {\n    \n    data() {\n      return {\n        messages: [],\n        likes: 0,\n        dislikes:0,\n        posts: {\n          title: null,\n          content: null,\n          loading: false\n          },\n        order: 1,\n        searchText: null,\n        ccn: null,\n        currentPage: 0,\n       messagesPerPage: 2,\n        resultCount: 0\n      }\n    },\n    created() {\n      axios\n        .get('http://localhost:3000/api/messages/')\n        .then(response => {\n          this.messages = response.data\n          console.log('je suis un yankee un');\n\n          })\n        \n        .catch(error => console.log(error()))\n    },\n    computed: {\n    totalPages: function() {\n          console.log('je suis un yankee deux');\n\n      console.log(Math.ceil(this.resultCount / this.messagesPerPage) + \"totalPages\");\n      return Math.ceil(this.resultCount / this.messagesPerPage);\n\n    }\n  },\n\n  filters: {\n    paginate: function(list) {\n        return  console.log('je suis un yankee trois');\n\n      // this.resultCount = this.messages.length;\n      // console.log(this.resultCount + \" Result count\");\n      // console.log(this.currentPage + \" current page\");\n      // console.log(this.messagesPerPage + \" messages per page\");\n      // console.log(this.totalPages + \" Total pages 2\");\n      // if (this.currentPage >= this.totalPages) {\n      //   this.currentPage = Math.max(0, this.totalPages - 1);\n      // }\n      // var index = this.currentPage * this.messagesPerPage;\n      // console.log(index + \" index\");\n      // console.log(this.messages.slice(index, index + this.messagesPerPage));\n      // return this.messages.slice(index, index + this.messagesPerPage);\n    }\n  },\n  methods: {\n      reset(){\n                this.$page.restart('first');\n            },\n      doLike: function (id) {\n        let objMySession = localStorage.getItem(\"obj\")\n        let myStorageToken = JSON.parse(objMySession)\n        let token = myStorageToken.myToken;\n        this.axios.post('http://localhost:3000/api/messages/' + id + '/vote/like', null, {\n            headers: {\n              'Authorization': token\n            }\n          }\n        )\n          .then(() => {\n            axios\n              .get('http://localhost:3000/api/messages/')\n              .then(response => this.messages = response.data)\n              .catch(error => console.log(error()))\n\n          })\n          .catch(error => console.log(error()))\n      },\n      doDislike: function (id) {\n        let objMySession = localStorage.getItem(\"obj\")\n        let myStorageToken = JSON.parse(objMySession)\n        let token = myStorageToken.myToken;\n        this.axios.post('http://localhost:3000/api/messages/' + id + '/vote/dislike', null, {\n            headers: {\n              'Authorization': token\n            }\n          }\n        )\n          .then(() => {\n            axios\n              .get('http://localhost:3000/api/messages/')\n              .then(response => this.messages = response.data)\n              .catch(error => console.log(error()))\n          })\n          .catch(error => console.log(error()))\n      },\n      postData(e) {\n        let objMySession = localStorage.getItem(\"obj\")\n        let myStorageToken = JSON.parse(objMySession)\n        let token = myStorageToken.myToken;\n        this.axios.post('http://localhost:3000/api/messages/new/', this.posts, {\n            headers: {\n              'Authorization': token\n            }\n          }\n        )\n          .then(() => {\n            this.posts = {\n              title: null,\n              content: null,\n            };\n            axios\n              .get('http://localhost:3000/api/messages/')\n              .then(response => this.messages = response.data)\n              .catch(error => console.log(error()))\n          })\n          .catch(error => console.log(error()))\n\n        e.preventDefault();\n      }\n    }\n  }\n</script>\n<style scoped>\n    .group {\n        height: 100%;\n        display: flex;\n        align-items: center;\n        justify-content: space-evenly;\n    }\n    .group__header__body {\n        padding: 1rem;\n        background-color: rgba(14, 14, 15, 0.205);\n        border-radius: 2rem;\n        margin-top: 1rem;\n        margin-bottom: 1rem;\n    }\n    .like-buttons[_ngcontent-cpa-c6] {\n        display: flex;\n    }\n    .dislikes[_ngcontent-cpa-c6], .likes[_ngcontent-cpa-c6] {\n        margin: 0 .3em;\n    }\n    a {\n  color: #999;\n}\n.current {\n  color: red;\n}\nul {\n  padding: 0;\n  list-style-type: none;\n}\nli {\n  display: inline;\n  margin: 5px 5px;\n}\n</style>"]}]}