uter-link>  \n                  <router-link class=\"nav__down__first\" v-else  to=\"/logout\">Logout</router-link> \n                  <router-link class=\"nav__down__first\" v-if= testBool  to=\"/compte\">Admin</router-link> \n                </div>\n        <router-view/>\n  </div>\n</template>\n\n<script>\nimport Vue from 'vue'\nimport axios from 'axios';\nimport VueAxios from 'vue-axios'\nVue.use(VueAxios, axios)\nimport { mapState } from 'vuex'\nexport default {\n  computed: {\n    ...mapState(['logged']),\n     testBool(){\n      return this.item.isAdmin\n    }\n    // ...mapGetters(['nowLogged'])\n  },\n    data() {\n    return {\n      item:''\n   \n    };\n  },\n   created() {\n     let objMySession = localStorage.getItem(\"obj\")\n     let myStorageToken = JSON.parse(objMySession)\n     let token = myStorageToken.myToken;\n    axios\n     .get('http://localhost:3000/api/users/me/',{\n            headers: {\n              'Authorization': token\n            }\n          })\n        .then(response => {\n          this.item = response.data\n\n          })\n        \n        .catch(error => console.log(error()))\n  },\n\n  props: {},\n  methods: {},\n \n};\n</script>\n<style>\n.nav{\n  padding: 20px;\n}\n.nav__in{\n  display:flex;\n margin: 0 auto;\nborder: 3px solid black;\n width: 320px;\nheight: 50px;\nborder-radius: 30px;\n}\n.nav__down{\n  padding: 20px;\n\n\n}\n.nav__down__first{\nborder: 3px solid black;\nwidth: auto;\nheight: auto;\nborder-radius: 30px;\nborder: 3px solid rgb(230, 122, 122);\nmargin: 5px;\npadding: 5px;\nfont-size: 10px;\nfont-weight: bold;\nbackground: rgb(231, 216, 216);\ntext-decoration:none;\n}\n\n\nimg {\n  width: 100%;\n  height: 100%;\n  border-radius: 30px;\n  margin:  auto;\n}\n</style>\n\n\n"]}]}